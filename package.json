{
  "name": "trello-study",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@openapitools/openapi-generator-cli": "^2.4.26",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.25.0",
    "global": "^4.4.0",
    "msw": "^0.39.1",
    "node-sass": "^7.0.1",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "5",
    "react-scripts": "^4.0.3",
    "react-transition-group": "^4.4.2",
    "redux": "^4.1.2",
    "redux-actions": "^2.6.5",
    "styled-components": "^5.3.3",
    "swagger-cli": "^4.0.4",
    "typescript": "^4.6.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "openapi": "npm run openapi:get && npm run openapi:prettier && npm run openapi:generate",
    "openapi:validate": "swagger-cli validate https://trello-api-gdh.herokuapp.com/v2/api-docs",
    "openapi:get": "curl https://trello-api-gdh.herokuapp.com/v2/api-docs > docs/api.json",
    "openapi:prettier": "prettier --write docs/api.json",
    "openapi:generate": "openapi-generator-cli generate -g typescript-axios -i docs/api.json -o src/api/generated --additional-properties=withoutPrefixEnums=false,enumUnknownDefaultCase=false,disallowAdditionalPropertiesIfNotPresent=false"
  },
  "homepage": "https://trello-fo.netlify.app/",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
